<% what_for = params[:minisymposium] ? 'Minisymposia' : 'Submissions' %>
<%= program_title(what_for + " Ratings", 'star') %>  

<%= render @list, condensed: true %>  

<% if @highlight > 0 %>  
  <%= javascript_tag do %>  
    $('document').ready(function() {
      $("#html_cs_<%= @highlight %> .panel-body").show();
      $("#html_cs_<%= @highlight %> .panel-footer").show();
      $('html, body').animate({ scrollTop: $("#html_cs_<%= @highlight %>").offset().top });
    });
  <% end %>
<% end %>

