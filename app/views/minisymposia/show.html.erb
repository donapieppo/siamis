<%= render @conference_session %>  

<% if user_in_scientific_commettee? %>  
  <% if rating = current_user.ratings.where(minisymposium: @conference_session).first %>  
    You rated <strong><%= rating.score %>/5 </strong> this Minisymposium.
  <% end %>

  <%= link_to_next_rating(Rating.get_unrated_minisymposium(current_user)) %>
<% elsif user_in_organizer_commettee? %>  
  <%= link_to_next_approval(Minisymposium.submitted.where.not(id: @conference_session.id).first) %>
<% end %>

