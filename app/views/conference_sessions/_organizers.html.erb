<div class="simple-form form-horizontal">
<div class="form-group string">
  <label class="col-sm-3 control-label string">
    <%= @conference_session.organizers_label %>
  </label>
  <div class="col-sm-9">
    <p>
    <% @conference_session.organizers.each do |organizer| %>  
      <%= organizer %> <%= link_to_delete organizer_path(organizer) %><br/>
    <% end %>
    </p>
    <p><%= link_to icon(:plus) + " add #{@conference_session.organizers_label.singularize}", new_conference_session_organizer_path(@conference_session), class: :button %></p>
  </div>
</div>
</div>



