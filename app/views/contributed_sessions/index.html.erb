<h1>Contributed Sessions</h1>

<ul class="collection">
<% ContributedSession.find_each do |contributed_session| %>  
  <li class="collection-item"><%= link_to contributed_session, contributed_session %> </li>
<% end %>

<% if user_committee_organizer? %>  
  <p class="centered">
    <%= link_to 'Create Contributed Session', new_contributed_session_path, class: :big_button %>  
  </p>
<% end %>
