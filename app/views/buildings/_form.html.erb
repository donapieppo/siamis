<%= simple_form_for @building do |building| %>  
  <%= building.error_notification %>
  <%= building.input :name %> 
  <%= building.input :description %> 
  <%= building.input :address %> 
  <%= building.input :city %> 
  <%= building.input :lat %> 
  <%= building.input :lng %> 
  <%= building.submit %>  
<% end %>

